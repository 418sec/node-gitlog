{
  "name": "gitlog",
  "version": "3.2.0",
  "description": "Git log parser for Node.JS",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.js test/**/*.js",
    "test": "nyc --reporter=text mocha -- -r should"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/domharrington/node-gitlog.git"
  },
  "keywords": [
    "git",
    "log",
    "parser"
  ],
  "author": "Dom Harrington <domharrington@protonmail.com>",
  "license": "BSD",
  "engines": {
    "node": ">= 10.x"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-xo": "^0.29.1",
    "eslint-plugin-jsdoc": "^22.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1",
    "prettier": "^2.0.4",
    "should": "~13.2.3"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "lodash.assign": "^4.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  },
  "nyc": {
    "branches": 85,
    "lines": 95,
    "functions": 100,
    "statements": 95
  }
}
